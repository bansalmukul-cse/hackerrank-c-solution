#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
;
   
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char ch;
    scanf("%c", &ch);
    printf("%c\n", ch);
    char s[20];
    scanf("%s",s);
    printf("%s\n",s);
    scanf("\n");
    char str[20]; 
    scanf("%[^\n]%*c", str); 
    printf("%s", str); 
  


    return 0;
}
